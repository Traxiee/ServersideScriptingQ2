<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= typeof title != 'undefined' ? title : 'nej' %></title>
    <link rel="stylesheet" href="/style.css">
</head>
<body>

<%- include('partials/nav.ejs') %>

    <% if(typeof moviess !='undefined' ) { moviess.forEach(element=> { %>
        <div class="fancy">
            <p>Title: <%= element.title%>
            </p>
            <%- element != undefined && element.imageName != undefined ? `<img src="/images/${element.imageName}" style="width:100px;"><br>` : '' %>
            
            <a href="/moviedetail/<%= element._id%>">Details</a>
        </div>
        <%}); } %>

</body>
</html>